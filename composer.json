{
    "name": "mk1/rest-client",
    "description": "simple REST client with cURL transport",
    "require": {
        "ext-curl": "*",
        "nyholm/psr7": "^1.4",
        "psr/http-client": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "RestClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RestClient\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "ext-json": "*",
        "phpunit/phpunit": "^9.5",
        "symfony/dotenv": "^4."
    }
}
